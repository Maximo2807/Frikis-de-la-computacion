algoritmo MontanaRusa
	definir altu como real;
	definir edad como entero;
	escribir "ingrese su edad";
	leer edad;
	escribir "ingrese su altura";
	leer altu;
	si (edad>17) y (altu >=1.70) entonces 
		escribir "usted puede ingresar a la montaña rusa";
	sino
		escribir "lo siento, no puede ingresar a la montaña rusa";
	finsi
finalgoritmo
